{# empty Twig template #}
{% extends "SiteFrontOfficeBundle::layout.html.twig" %}
{% block body %}
<div class="container">
<div class="main">
    <div class="registration">
<div class="registration_left">

    {% render url('fos_user_registration_register') %} 


      </div>  
        <div class="registration_left">
                <h2>existing user</h2>
                {% block fos_user_content %}
                    

                {% if error %}
                    <div>{{ error.messageKey|trans(error.messageData, 'security') }}</div>
                {% endif %}
                 <div class="registration_form">
                 <!-- Form -->
                        <form id="registration_form" action="{{ path("fos_user_security_check") }}" method="post">
                            <input type="hidden" name="_csrf_token" value="{{ csrf_token }}" />

                            <div>
                                        <label>
                                                <input placeholder="email:" type="email" tabindex="3" id="username" name="_username" value="{{ last_username }}" required="required" />
                                        </label>
                                </div>
                                <div>
                                        <label>
                                            <input placeholder="password" type="password" id="password" name="_password" tabindex="4" required="required" />
                                        </label>
                                </div>						
                                <div>
                                    <input type="submit" id="_submit" name="_submit" value="{{ 'security.login.submit'|trans }}" />
                                </div>
                                <div class="forget">
                                        <a href="#">forgot your password</a>
                                </div>
                        </form>
                        <!-- /Form -->
                        </div>
                    {% endblock fos_user_content %}
        </div>
        <div class="clearfix"></div>

</div>
        </div>
{% endblock %}